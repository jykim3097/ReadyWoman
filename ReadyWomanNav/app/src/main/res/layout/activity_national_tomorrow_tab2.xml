                </TableRow>

                <TableRow
                    android:id="@+id/tomorrow2_defTable3_tr3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp">
                    <TextView
                        android:id="@+id/tomorrow2_defTable3_tr3_td1"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_margin="5dp"
                        android:text="@string/blank"
                        android:background="#a839a8" />
                    <TextView
                        android:id="@+id/tomorrow2_defTable3_tr3_td2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/tomorrow2_defTable3_tr3_td2"/>
                </TableRow>

            </TableLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/tomorrow2_distribution"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp">

            <!--서울특별시 여성인력개발기관 분포도-->
            <TextView
                android:id="@+id/tomorrow2_distributionName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="@string/tomorrow2_distribution_name"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="#a839a8"
                android:background="#f9eff9"/>

            <ImageView
                android:id="@+id/tomorrow2_distribution_img"
                android:layout_width="wrap_content"
                android:layout_height="250dp"
                android:layout_margin="5dp"
                android:src="@drawable/wm_int_3"
                android:scaleType="fitStart"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/tomorrow2_contact"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp">

            <!--서울특별시 여성능력개발원 연락처-->
            <TextView
                android:id="@+id/tomorrow2_contactName1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="@string/tomorrow2_contact_name1"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textColor="#a839a8"
                android:background="#f9eff9"/>

            <TextView
                android:id="@+id/tomorrow2_contactName1_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:text="@string/tomorrow2_contactName1_title"
                android:textSize="15sp"
                android:textStyle="bold"
                android:textColor="#a839a8"/>

            <TableLayout
                android:id="@+id/tomorrow2_contactTable1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:stretchColumns="1,2"
                android:shrinkColumns="2">

                <TableRow
                    android:id="@+id/tomorrow2_contactTable1_tr1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp">
                    <TextView
                        android:id="@+id/tomorrow2_contactTable1_tr1_td1"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_margin="5dp"
                        android:text="@string/blank"
                        android:background="#a839a8" />
                    <TextView
                        android:id="@+id/tomorrow2_contactTable1_tr1_td2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tomorrow2_contactTable1_tr1_td2"/>
                </TableRow>

                <TableRow
                    android:id="@+id/tomorrow2_contactTable1_tr2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp">
                    <TextView
                        android:id="@+id/tomorrow2_contactTable1_tr2_td1"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_margin="5dp"
                        android:text="@string/blank"
                        android:background="#f9eff9"/>
                    <TextView
