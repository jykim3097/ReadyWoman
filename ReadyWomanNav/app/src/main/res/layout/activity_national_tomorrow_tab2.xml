                        android:id="@+id/tomorrow2_defTable3_tr1_td2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@string/tomorrow2_defTable3_tr1_td2"/>
                </TableRow>

                <TableRow
                    android:id="@+id/tomorrow2_defTable3_tr2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="5dp">
                    <TextView
                        android:id="@+id/tomorrow2_defTable3_tr2_td1"
                        android:layout_width="5dp"
