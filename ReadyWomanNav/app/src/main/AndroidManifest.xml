<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.readywoman.codef.readywomannav">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_readywoman"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_readywoman_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden|adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailActivity"></activity>
        <activity android:name=".NationalTomorrowActivity"></activity>
        <activity android:name=".NationalEmployeecardActivity"></activity>
        <activity android:name=".NationalSeoulsupportActivity"></activity>
        <activity android:name=".NationalMogefsupportActivity"></activity>
        <activity android:name=".NationalEtcsupportActivity"></activity>
        <activity android:name=".NationalCollectiveconsultationActivity"></activity>
    </application>

</manifest>